<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">


    <changeSet id="add-columns-into-employee-registration" author="efremov">
        <addColumn tableName="employee_registration">
            <column name="probation_period" type="INT"/>
            <column name="resource_comment" type="VARCHAR(255)"/>
            <column name="operating_system" type="VARCHAR(64)"/>
            <column name="additional_soft" type="VARCHAR(255)"/>
        </addColumn>
    </changeSet>
</databaseChangeLog>

