<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet author="turik" id="deleteRoleIdFromUserLoginTable">
        <dropForeignKeyConstraint baseTableName="user_login" constraintName="FK_ULOGIN_ROLE"/>
        <dropColumn tableName="user_login" columnName="roleId"/>
    </changeSet>

    <changeSet author="turik" id="deleteRoleIdFromUserSessionTable">
        <dropColumn tableName="user_session" columnName="role_id"/>
    </changeSet>

</databaseChangeLog>
