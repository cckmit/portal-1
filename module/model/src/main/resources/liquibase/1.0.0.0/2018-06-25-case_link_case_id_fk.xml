<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="add-fk-case-link-to-case-object" author="buchmastov">
        <addForeignKeyConstraint constraintName="case_link_case_id_fk"
                                 baseTableName="case_link" baseColumnNames="case_id"
                                 referencedTableName="case_object" referencedColumnNames="id"
                                 onDelete="CASCADE" onUpdate="CASCADE"/>
    </changeSet>

</databaseChangeLog>