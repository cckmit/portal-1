<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="add-fired_date-to-worker_entry-table" author="porubov">
        <addColumn tableName="worker_entry">
            <column name="fired_date" type="DATETIME"/>
        </addColumn>
        <addColumn tableName="worker_entry">
            <column name="is_deleted" type="BIT(1)"/>
        </addColumn>
        <addColumn tableName="worker_entry">
            <column name="is_need_migration_at_fire" type="BIT(1)"/>
        </addColumn>
    </changeSet>
</databaseChangeLog>
