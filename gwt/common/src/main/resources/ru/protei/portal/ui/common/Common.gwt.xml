<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='common'>
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.inject.Inject" />

    <inherits name="ru.brainworm.factory.generator.injector.Injector"/>
    <inherits name="ru.brainworm.factory.generator.activity.Activity"/>

    <inherits name="ru.brainworm.factory.core.datetimepicker.Datetimepicker"/>
    <inherits name="ru.brainworm.factory.widget.table.Table"/>
    <inherits name="ru.brainworm.factory.widget.dataset.Dataset"/>

    <extend-property name="locale" values="ru" />
    <extend-property name="locale" values="en" />

    <set-property-fallback name="locale" value="ru"/>
    <set-property name="locale" value="en,ru"/>

    <inherits name="com.google.gwt.logging.Logging"/>
    <set-property name="gwt.logging.logLevel" value="FINEST"/>
    <set-property name="gwt.logging.enabled" value="TRUE"/>
    <set-property name="gwt.logging.consoleHandler" value="ENABLED" />

    <source path='client'/>
    <source path='shared'/>
</module>
