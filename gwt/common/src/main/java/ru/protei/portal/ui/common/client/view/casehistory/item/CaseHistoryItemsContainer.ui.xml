<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
>

    <ui:with field="lang" type="ru.protei.portal.ui.common.client.lang.Lang" />

    <g:HTMLPanel tag="li" styleName="row form-group">
        <div class="col-md-1">
            <span class="history-time-icon fas fa-history fa-lg"/>
        </div>
        <div class="col-md-11">
            <span ui:field="initiator" class="float-left m-r-10 font-weight-bold"/>
            <div class="text-right">
                <span class="case-history-item-date" ui:field="date"/>
            </div>
        </div>
        <g:HTMLPanel styleName="col-md-12" ui:field="itemsContainer"/>
    </g:HTMLPanel>
</ui:UiBinder>
