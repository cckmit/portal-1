<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <g:HTMLPanel ui:field="notify" styleName="notify">
        <div class="body">
            <span class="icon-mIcon"></span>
            <span ui:field="message" class="text"></span>
            <span ui:field="timer" class="timer"></span>
        </div>
    </g:HTMLPanel>


</ui:UiBinder>