<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel>

        <g:FormPanel ui:field="form">
            <g:HTMLPanel tag="label">
                <g:FileUpload ui:field="fileUpload" styleName="hide" name="file"/>
                <g:HTMLPanel ui:field="visibleContent" styleName="visible-content" />
            </g:HTMLPanel>
        </g:FormPanel>

    </g:HTMLPanel>
</ui:UiBinder>
