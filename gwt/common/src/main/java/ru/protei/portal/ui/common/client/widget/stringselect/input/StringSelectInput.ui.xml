<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel styleName="select2-container select2-container-multi" ui:field="widgetContainer">
        <g:HTMLPanel ui:field="itemContainer" tag="ul" styleName="select2-choices">
        </g:HTMLPanel>
        <g:HTMLPanel ui:field="inputContainer" styleName="select2-search-field flex-parent">
            <g:TextBox ui:field="input" styleName="select2-input"/>
            <g:HTMLPanel styleName="select2-button">
                <g:Anchor ui:field="add" href="#" visible="false" styleName="select2-button-add p-t-5"/>
            </g:HTMLPanel>
        </g:HTMLPanel>
    </g:HTMLPanel>
</ui:UiBinder>