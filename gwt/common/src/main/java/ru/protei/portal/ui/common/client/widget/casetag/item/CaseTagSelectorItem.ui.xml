<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field="lang" type="ru.protei.portal.ui.common.client.lang.Lang" />

    <g:HTMLPanel ui:field="panel" tag="li" styleName="case-tag">
        <g:InlineLabel ui:field="icon" styleName="case-tag-icon"/>
        <g:InlineLabel ui:field="text" styleName="case-tag-text"/>
        <g:InlineLabel ui:field="companyName" styleName="case-tag-company-name"/>
        <g:InlineLabel ui:field="editIcon" styleName="case-tag-edit"/>
    </g:HTMLPanel>
</ui:UiBinder>
