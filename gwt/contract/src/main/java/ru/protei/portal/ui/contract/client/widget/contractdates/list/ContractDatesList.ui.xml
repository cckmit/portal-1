<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field="lang" type="ru.protei.portal.ui.common.client.lang.Lang"/>

    <g:HTMLPanel>
        <g:HTMLPanel styleName="col-md-12" ui:field="costOverflowWarning" visible="false">
            <div class="col-md-12 alert alert-danger">
                <strong><ui:text from="{lang.attention}"/>: </strong><ui:text from="{lang.contractDatesWarningCostOverflow}"/>
            </div>
        </g:HTMLPanel>
        <g:FlowPanel ui:field="container" styleName="form-group col-md-12 m-b-0"/>
        <div class="form-group col-md-12">
            <div class="pull-right">
                <g:Button ui:field="add" styleName="btn btn-xs btn-info" text="{lang.buttonAdd}"/>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>