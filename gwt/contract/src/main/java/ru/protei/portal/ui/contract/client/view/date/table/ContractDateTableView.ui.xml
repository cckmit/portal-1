<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:table="urn:import:ru.brainworm.factory.widget.table.client">

    <ui:with field="lang" type="ru.protei.portal.ui.common.client.lang.Lang" />

    <g:HTMLPanel>
        <div class="row">
            <g:HTMLPanel styleName="col-md-12" ui:field="costOverflowWarning" visible="false">
                <div class="col-md-12 alert alert-danger">
                    <strong><ui:text from="{lang.attention}"/>: </strong><ui:text from="{lang.contractDatesWarningCostOverflow}"/>
                </div>
            </g:HTMLPanel>
            <g:HTMLPanel ui:field="tableContainer" styleName="col-md-12">
                <table:TableWidget ui:field="table" styleName="table-hover "/>
            </g:HTMLPanel>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>