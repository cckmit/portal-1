<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field="lang" type="ru.protei.portal.ui.common.client.lang.Lang" />

    <g:HTMLPanel styleName="OrganizationPassport">
        <div class="mainInfo">
            <div class="t_wrapper">
                <div class="tc_wrapper">
                    <div class="section info">
                        <div class="head">
                            <span class="header"><ui:text from="{lang.companyCommonHeader}"/></span>
                        </div>
                        <div class="body">
                            <div id="scrollInfo" class="wrapper white-ps-container ps-container ps-active-x"
                                 data-ps-id="f1d17374-ad8a-c76e-3b0f-4c17c324df6b">
                                <div class="t_wrapper">
                                    <div class="tr_wrapper">
                                        <div class="tc_wrapper header"><ui:text from="{lang.contactLastName}"/></div>
                                        <label class="tc_wrapper content" ui:field="lastName"></label>
                                    </div>
                                    <div class="tr_wrapper">
                                        <div class="tc_wrapper header"><ui:text from="{lang.contactFirstName}"/></div>
                                        <label class="tc_wrapper content" ui:field="firstName"></label>
                                    </div>
                                    <div class="tr_wrapper">
                                        <div class="tc_wrapper header"><ui:text from="{lang.contactSecondName}"/></div>
                                        <label class="tc_wrapper content" ui:field="secondName"></label>
                                    </div>
                                    <div class="tr_wrapper">
                                        <div class="tc_wrapper header"><ui:text from="{lang.gender}"/></div>
                                        <label class="tc_wrapper content" ui:field="sex"></label>
                                    </div>
                                    <div class="tr_wrapper">
                                        <div class="tc_wrapper header"><ui:text from="{lang.birthday}"/></div>
                                        <label class="tc_wrapper content" ui:field="birthday"></label>
                                    </div>
                                    <div class="tr_wrapper">
                                        <div class="tc_wrapper header"><ui:text from="{lang.contactPosition}"/></div>
                                        <label class="tc_wrapper content" ui:field="position"></label>
                                    </div>
                                    <div class="tr_wrapper">
                                        <div class="tc_wrapper header"><ui:text from="{lang.contactDepartment}"/></div>
                                        <label class="tc_wrapper content" ui:field="department"></label>
                                    </div>
                                    <div class="tr_wrapper">
                                        <div class="tc_wrapper header"><ui:text from="{lang.phone}"/></div>
                                        <label class="tc_wrapper content" ui:field="phone"></label>
                                    </div>
                                    <div class="tr_wrapper">
                                        <div class="tc_wrapper header"><ui:text from="{lang.fax}"/></div>
                                        <label class="tc_wrapper content" ui:field="fax"></label>
                                    </div>
                                    <div class="tr_wrapper">
                                        <div class="tc_wrapper header"><ui:text from="{lang.email}"/></div>
                                        <label class="tc_wrapper content" ui:field="email"></label>
                                    </div>
                                    <div class="tr_wrapper">
                                        <div class="tc_wrapper header"><ui:text from="{lang.address}"/></div>
                                        <label class="tc_wrapper content" ui:field="address"></label>
                                    </div>
                                    <div class="tr_wrapper">
                                        <div class="tc_wrapper header"><ui:text from="{lang.comment}"/></div>
                                        <label class="tc_wrapper content" ui:field="info"></label>
                                    </div>
                                    <div class="tr_wrapper">
                                        <div class="tc_wrapper header"><ui:text from="{lang.linkToObject}"/></div>
                                        <label class="tc_wrapper content" ui:field="link"></label>
                                    </div>

                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>