<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field="lang" type="ru.protei.portal.ui.common.client.lang.Lang" />

    <g:HTMLPanel ui:field="root">
        <div class="footer" style="padding-bottom: 0px">
        <g:Button ui:field="add" styleName="btn btn-primary">
            <ui:text from="{lang.buttonAdd}"/>
        </g:Button>
        </div>
        <g:HTMLPanel ui:field="list" styleName="flex-container">
            <g:HTMLPanel ui:field="pamrList" styleName="flex-split-vert-item"></g:HTMLPanel>
            <g:HTMLPanel ui:field="pdraList" styleName="flex-split-vert-item"></g:HTMLPanel>
        </g:HTMLPanel>
    </g:HTMLPanel>
</ui:UiBinder>