<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <g:HTMLPanel ui:field="root" tag="li" styleName="row form-group">
        <div class="col-md-10">
            <i ui:field="approve" class="fas fa-lg m-r-10"/>
            <span ui:field="decimalNumber" class="decimal-number m-r-10"/>
            <span ui:field="information" class="information"/>
        </div>
        <div class="col-md-2">
            <g:Anchor ui:field="download" styleName="fas fa-lg fa-file-download" href="#"/>
            <g:Anchor ui:field="edit" styleName="m-l-10 fa fa-lg fa-edit" href="#"/>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>