<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel tag="li" styleName="organization list-group-item">
        <div class="wrapper">
            <div ui:field="type" class="type"/>
            <div ui:field="name" class="name"/>
            <g:PushButton ui:field="menuButton" styleName="menu_button"/>
            <g:PushButton ui:field="favoriteButton" styleName="favorite_button"/>
        </div>
        <g:HTMLPanel ui:field="previewContainer" styleName="list-group-expander"/>
    </g:HTMLPanel>
</ui:UiBinder>